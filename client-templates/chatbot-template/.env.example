# Chatbot Template Environment Variables
# Copy this file to .env.local and fill in your values

# =============================================================================
# Supabase (Required)
# =============================================================================
NEXT_PUBLIC_SUPABASE_URL=your-supabase-project-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# =============================================================================
# n8n Webhook (Required)
# =============================================================================
# Your n8n webhook URL for the chat workflow
N8N_WEBHOOK_URL=https://your-n8n-instance.com/webhook/your-workflow-id

# =============================================================================
# Voice Chat - VAPI (Optional)
# =============================================================================
# Leave empty if voice chat is not needed
NEXT_PUBLIC_VAPI_PUBLIC_KEY=
NEXT_PUBLIC_VAPI_ASSISTANT_ID=
